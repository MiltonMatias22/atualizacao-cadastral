    		    <action android:name="android.intent.action.DEFAULT"/>
    		    <category android:name="android.intent.category.DEFAULT"/>
    		</intent-filter>    
    	</activity>
        
    	<activity android:name="com.br.gsanac.view.RelatorioActivity"
    	    android:screenOrientation="portrait" >
    	    <intent-filter>
    	        <action android:name="android.intent.action.DEFAULT" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
    	</activity>
    	
    	<activity android:name="com.br.gsanac.view.RelatorioPorOcorrenciaCadastroActivity"
    	    android:screenOrientation="portrait" >
    	    <intent-filter>
    	        <action android:name="android.intent.action.DEFAULT" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
    	</activity>
    	
    	<activity 
    	    android:name="com.br.gsanac.view.MainActivity"
    	    android:screenOrientation="portrait" >
    		<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
    		    <category android:name="android.intent.category.LAUNCHER"/>
    		</intent-filter>
    	</activity>
    	
    	<activity 
    	    android:name="com.br.gsanac.view.DownloadApkActivity"
    	    android:screenOrientation="portrait" >
