                <action android:name="android.intent.action.DEFAULT" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
    	</activity>
    	
    	<activity
    	    android:name="com.br.gsanac.view.LogradouroInserirActivity"
    	    android:screenOrientation="portrait"
    	    android:label="@string/cabecalho_logradouro" >
    	    <intent-filter>
                <action android:name="android.intent.action.DEFAULT" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
    	</activity>
    	
    	<activity
    	    android:name="com.br.gsanac.view.MapaActivity"
    	    android:screenOrientation="portrait"
    	    android:label="@string/cabecalho_mapa" >
    	    <intent-filter>
    	        <action android:name="android.intent.action.DEFAULT"/>
    		    <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
    	</activity>
    	
    	<activity 
    	    android:name="com.br.gsanac.view.RoteiroActivity"
    	    android:screenOrientation="portrait" >
