    	        <category android:name="android.intent.category.DEFAULT"/>
    	    </intent-filter>
    	</activity>
    	
    	<activity 
    	    android:name="com.br.gsanac.view.DownloadArquivoTXTActivity"
    	    android:screenOrientation="portrait">
    	    <intent-filter>
    	        <action android:name="android.intent.action.DEFAULT"/>
    	        <category android:name="android.intent.category.DEFAULT"/>
    	    </intent-filter>
    	</activity>
    		
    </application>
</manifest>
